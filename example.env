# Web socket settings
WEB_SOCKET_PING_INTERVAL=10000
WEB_SOCKET_MAX_PONG_FAILS=3

# Database connection
DB_CONNECTION_URL=postgres://username:password@localhost:5432/database_name

# Environment mode
NODE_ENV=production

# TOTP secret for authentication
TOTP_SECRET=YOUR_TOTP_SECRET
BYPASS_AUTH=1

# Meter value timeout
METER_VALUE_TIMEOUT=300

# Server port
PORT=3000

# Webhook URLs
STATUS_CHANGE_WEBHOOK_URL=https://yourserver.com/connector-status-change
TRANSACTION_COMPLETION_WEBHOOK_URL=https://yourserver.com/transaction-completed

